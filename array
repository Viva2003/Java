package exercise;

import java.util.Scanner;

public class Array {
    public static void main(String[] args){
        System.out.println("Enter array size");
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int c[]=new int[n];
        for(int i=0;i<n;i++){
        c[i]=sc.nextInt();
    }
        System.out.println("The entered array is");
       for(int i=0;i<n;i++){
        System.out.println(c[i]+"");
    }

}
}
